\begin_layout Enumerate
\begin_inset Formula $\gamma\left(t\right)=\left(2t^{3},2t^{2}+1\right)$
\end_inset

, 
\begin_inset Formula $\overline{f}\left(x,y\right)=\left(x+y,x-y\right)$
\end_inset

.
\begin_inset Formula 
\[
\int_{\gamma}\overline{f}=\int_{0}^{1}\left\langle f\left(\gamma\left(t\right)\right)\mid\gamma'\left(t\right)\right\rangle dt
\]

\end_inset


\begin_inset Formula 
\[
\gamma'\left(t\right)=\left(6t^{2},4t\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\int_{\gamma}\overline{f} & =\int_{0}^{1}\left\langle \left(2t^{3}+2t^{2}+1,2t^{3}-2t^{2}-1\right)\mid\left(6t^{2},4t\right)\right\rangle dt=\\
= & \int_{0}^{1}\left(2t^{3}+2t^{2}+1\right)6t^{2}+\left(2t^{3}-2t^{2}-1\right)4tdt\\
= & \int_{0}^{1}12t^{5}+12t^{4}+6t^{2}+8t^{4}-8t^{3}-4tdt=\\
= & \int_{0}^{1}12t^{5}+20t^{4}-8t^{3}+6t^{2}-4tdt=\\
= & \left[2t^{6}+4t^{5}-2t^{4}+2t^{3}-2t^{2}\right]_{0}^{1}=2+4-2+2-2=4
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\overline{f}\left(x,y\right)=\left(\cos\left(2y\right),-2x\sin\left(2y\right)\right)$
\end_inset

, 
\begin_inset Formula $\gamma\left(t\right)=\left(1+\frac{t}{\pi},t\right)$
\end_inset

.
\begin_inset Formula 
\[
\gamma'\left(t\right)=\left(\frac{1}{\pi},1\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\int_{\gamma}\overline{f} & =\int_{0}^{\frac{\pi}{2}}\left\langle \left(\cos2t,-2\left(1+\frac{t}{\pi}\right)\sin\left(2t\right)\right)\mid\left(\frac{1}{\pi},1\right)\right\rangle dt=\\
= & \int_{0}^{\frac{\pi}{2}}\frac{\cos2t}{\pi}-2\left(1+\frac{t}{\pi}\right)\sin2tdt=\\
= & \int_{0}^{\frac{\pi}{2}}\frac{\cos2t}{\pi}-2\sin2t-\frac{2t}{\pi}\sin2tdt=\\
= & \left[\frac{\sin2t}{2\pi}+\cos2t+\frac{1}{\pi}\left(t\cos2t-\sin2t\right)\right]_{0}^{\frac{\pi}{2}}=\\
= & \left(\frac{0}{2\pi}+\left(-1\right)+\frac{1}{\pi}\left(\frac{\pi}{2}\cdot\left(-1\right)-\left(0\right)\right)\right)-\left(\frac{0}{2\pi}+1+\frac{1}{\pi}\left(0\cdot\left(1\right)-0\right)\right)=\\
= & -1-\frac{1}{2}-1=-2\frac{1}{2}
\end{align*}

\end_inset


