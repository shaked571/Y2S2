\begin_layout Standard
\begin_inset Formula $\gamma$
\end_inset

 היא ספרית ולכן 
\begin_inset Formula $\Vert\gamma\left(t\right)\Vert=1$
\end_inset

, משמע 
\begin_inset Formula $\left\langle \gamma\left(t\right)\mid\gamma\left(t\right)\right\rangle $
\end_inset

 קבועה.
 לכן
\begin_inset Formula 
\[
\left(\left\langle \gamma\left(t\right)\mid\gamma\left(t\right)\right\rangle \right)'=2\left(\gamma_{1}\left(t\right)\gamma_{1}'\left(t\right)+\gamma_{2}\left(t\right)\gamma_{2}'\left(t\right)+\gamma_{3}\left(t\right)\gamma_{3}'\left(t\right)\right)=0
\]

\end_inset

נגזור שוב ונקבל כי
\begin_inset Formula 
\begin{align*}
0 & =\gamma_{1}'^{2}\left(t\right)+\gamma_{1}\left(t\right)\gamma_{1}^{''}\left(t\right)+\gamma_{2}'^{2}\left(t\right)+\gamma_{2}\left(t\right)\gamma_{2}^{''}\left(t\right)+\gamma_{3}'^{2}\left(t\right)+\gamma_{3}\left(t\right)\gamma_{3}^{''}\left(t\right)=\\
= & \Vert\gamma'\left(t\right)\Vert^{2}+\left\langle \gamma\left(t\right)\mid\gamma''\left(t\right)\right\rangle =1+\left\langle \gamma\left(t\right)\mid\gamma''\left(t\right)\right\rangle 
\end{align*}

\end_inset

קיבלנו כי
\begin_inset Formula 
\[
1=\left|\left\langle \gamma\left(t\right)\mid\gamma''\left(t\right)\right\rangle \right|\leq\Vert\gamma\left(t\right)\Vert\cdot\Vert\gamma''\left(t\right)\Vert=\Vert\gamma''\left(t\right)\Vert
\]

\end_inset

כנדרש.
