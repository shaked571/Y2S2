\begin_layout Standard
\begin_inset Formula $b\in\mathbb{R}^{n}$
\end_inset

, 
\begin_inset Formula $A\in O_{n}\left(\mathbb{R}\right)$
\end_inset

.
 
\begin_inset Formula $T\colon\mathbb{R}^{n}\to\mathbb{R}^{n}$
\end_inset

, 
\begin_inset Formula $Tx=Ax+b$
\end_inset

.
 
\begin_inset Formula $\delta\left(t\right)=T\left(\gamma\left(t\right)\right)$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
\delta'\left(t\right)=\left(T\left(\gamma\left(t\right)\right)\right)'=T'\left(\gamma\left(t\right)\right)\cdot\gamma'\left(t\right)=A\cdot\gamma'\left(t\right)
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
\int_{a}^{b}\Vert\delta'\left(t\right)\Vert dt=\int_{a}^{b}\Vert A\cdot\gamma'\left(t\right)\Vert dt=\int_{a}^{b}\Vert\gamma'\left(t\right)\Vert dt
\]

\end_inset

כי
\begin_inset Formula 
\begin{align*}
\Vert A\cdot\gamma'\left(t\right)\Vert & =\sqrt{\left(A\cdot\gamma'\left(t\right)\right)\cdot_{\text{scalar}}\left(A\cdot\gamma'\left(t\right)\right)}=\sqrt{\left(A\cdot\gamma'\left(t\right)\right)^{t}\cdot_{\text{matrix}}\left(A\cdot\gamma'\left(t\right)\right)}=\\
= & \sqrt{\gamma'\left(t\right)^{t}\cdot A^{t}\cdot A\cdot\gamma'\left(t\right)}=\sqrt{\gamma'\left(t\right)^{t}\cdot\gamma'\left(t\right)}=\Vert\gamma'\left(t\right)\Vert
\end{align*}

\end_inset


\end_layout

\begin_layout Section*
שאלה שביעית
\end_layout

\begin_layout Standard
\begin_inset FormulaMacro
\newcommand{\norm}[1]{\Vert#1\Vert}
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $M\in\text{GL}_{n}\left(\mathbb{R}\right)$
\end_inset

, 
\begin_inset Formula $\gamma\colon\left[a,b\right]\to\mathbb{R}^{n}$
\end_inset

 רגולרית, 
\begin_inset Formula $\delta\left(t\right)=M\gamma\left(t\right)$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
\delta'\left(t\right)=\left(M\gamma\left(t\right)\right)'=M'\left(\gamma\left(t\right)\right)\gamma'\left(t\right)=M\gamma'\left(t\right)
\]

\end_inset

כיוון ש-
\begin_inset Formula $M$
\end_inset

 הפיכה, 
\begin_inset Formula $\gamma'\left(t\right)\neq0\Rightarrow M\gamma'\left(t\right)\neq0$
\end_inset

.
 בפרט לכל 
\begin_inset Formula $t\in\left[a,b\right]$
\end_inset

,
\begin_inset Formula 
\[
\Vert\delta'\left(t\right)\Vert=\Vert M\gamma'\left(t\right)\Vert>0
\]

\end_inset

כיוון שזו פונקציה רציפה
\begin_inset Formula 
\[
Len\left(\delta\right)=\int_{a}^{b}\norm{\delta'\left(t\right)}dt>0
\]

\end_inset


\end_layout

\begin_layout Enumerate
נשים לב כי מתקיים
\begin_inset Formula 
\[
\norm{\delta\left(t\right)}=\norm{M\gamma'\left(t\right)}\leq\norm M_{\text{op}}\norm{\gamma'\left(t\right)}
\]

\end_inset

ו-
\begin_inset Formula 
\[
\norm{\gamma'\left(t\right)}=\norm{M^{-1}\cdot M\gamma'\left(t\right)}\leq\norm{M^{-1}}_{\text{op}}\norm{M\gamma'\left(t\right)}\iff\norm{M\gamma'\left(t\right)}\geq\norm{M^{-1}}_{\text{op}}^{-1}\norm{\gamma'\left(t\right)}
\]

\end_inset

סה"כ
\begin_inset Formula 
\[
\norm{M^{-1}}_{\text{op}}^{-1}\norm{\gamma'\left(t\right)}\leq\norm{\delta'\left(t\right)}\leq\norm M_{\text{op}}\norm{\gamma'\left(t\right)}
\]

\end_inset

והשאר מתקבל ממונוטוניות האינטגרל.
\end_layout

