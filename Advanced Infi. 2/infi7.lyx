\begin_layout Enumerate
צ"ל: 
\begin_inset Formula $\Vert u\times v\Vert=\Vert u\Vert\cdot\Vert v\Vert\cdot\sin\alpha$
\end_inset

.
 הוכחה:\SpecialChar softhyphen
\SpecialChar softhyphen

\begin_inset Formula 
\begin{align*}
\Vert u\Vert\cdot\Vert v\Vert\cdot\sin\alpha & =\sqrt{\left(u\cdot u\right)\cdot\left(v\cdot v\right)\cdot\left(1-\cos^{2}\alpha\right)}=\\
= & \sqrt{\left(u\cdot u\right)\cdot\left(v\cdot v\right)\cdot\left(1-\frac{\left(u\cdot v\right)^{2}}{\left(u\cdot u\right)\cdot\left(v\cdot v\right)}\right)}=\\
= & \sqrt{\left(u\cdot u\right)\cdot\left(v\cdot v\right)-\left(u\cdot v\right)^{2}}=\\
= & \sqrt{\left(u_{1}^{2}+u_{2}^{2}+u_{3}^{2}\right)\cdot\left(v_{1}^{2}+v_{2}^{2}+v_{3}^{2}\right)-\left(u_{1}v_{1}+u_{2}v_{2}+u_{3}v_{3}\right)^{2}}=\\
= & \sqrt{\substack{u_{1}^{2}v_{1}^{2}+u_{1}^{2}v_{2}^{2}+u_{1}^{2}v_{3}^{2}+u_{2}^{2}v_{1}^{2}+u_{2}^{2}v_{2}^{2}+u_{2}^{2}v_{3}^{2}+u_{3}^{2}v_{1}^{2}+u_{3}^{2}v_{2}^{2}+u_{3}^{2}v_{3}^{2}-\\
\left(\left(u_{1}v_{1}\right)^{2}+\left(u_{2}v_{2}\right)^{2}+\left(u_{3}v_{3}\right)^{2}+2u_{2}v_{2}u_{1}v_{1}+2u_{3}v_{3}u_{1}v_{1}+2u_{2}v_{2}u_{3}v_{3}\right)
}
}\\
= & \sqrt{u_{2}^{2}v_{3}^{2}-2u_{2}v_{3}u_{3}v_{2}+u_{3}^{2}v_{2}^{2}+u_{1}^{2}v_{3}^{2}-2u_{1}v_{3}u_{3}v_{1}+u_{3}^{2}v_{1}^{2}+u_{1}^{2}v_{2}^{2}-2u_{1}v_{2}u_{2}v_{1}+u_{2}^{2}v_{1}^{2}}\\
= & \sqrt{\left(u_{2}v_{3}-u_{3}v_{2}\right)^{2}+\left(-\left(u_{1}v_{3}-u_{3}v_{1}\right)\right)^{2}+\left(u_{1}v_{2}-u_{2}v_{1}\right)^{2}}=\\
= & \Vert u\times v\Vert
\end{align*}

\end_inset


