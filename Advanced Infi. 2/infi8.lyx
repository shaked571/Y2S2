\begin_layout Enumerate
\begin_inset FormulaMacro
\newcommand{\norm}[1]{\Vert#1\Vert}
\end_inset

תהי 
\begin_inset Formula $A\in O_{3}\left(\mathbb{R}\right)$
\end_inset

 ונתון כי 
\begin_inset Formula $u\times v=w$
\end_inset

, וגם 
\begin_inset Formula $\left(Au\right)\times\left(Av\right)=k$
\end_inset

.
 נזכור כי 
\begin_inset Formula $A$
\end_inset

 איזומטריה ולכן משמרת מכפלה פנימית 
\begin_inset Formula $\left\langle Au\mid Av\right\rangle =\left\langle u\mid v\right\rangle $
\end_inset

.
 בפרט 
\begin_inset Formula $\norm v=\norm{Av}$
\end_inset

.
 כעת,
\begin_inset Formula 
\begin{align*}
\norm w & =\norm{u\times v}=\norm u\norm v\sin\theta_{u,v}=\norm u\norm v\sqrt{1-\cos^{2}\theta}=\\
= & \norm{Au}\norm{Av}\sqrt{1-\frac{\left\langle u\mid v\right\rangle ^{2}}{\norm u^{2}\norm v^{2}}}=\norm{Au}\norm{Av}\sqrt{1-\frac{\left\langle Au\mid Av\right\rangle ^{2}}{\norm{Au}^{2}\norm{Av}^{2}}}=\\
= & \norm{Au\times Av}=\norm k
\end{align*}

\end_inset

 בפרט, 
\begin_inset Formula $k=\pm Aw$
\end_inset

, כי נובע ש-
\begin_inset Formula $Aw\perp Au,Av$
\end_inset

.
\end_layout

