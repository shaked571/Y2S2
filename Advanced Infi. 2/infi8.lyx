\begin_layout Standard
\begin_inset Formula $\gamma\left(t\right)=\left(a\cos\left(\frac{t}{c}\right),a\sin\left(\frac{t}{c}\right),b\cdot\frac{t}{c}\right)$
\end_inset

, 
\begin_inset Formula $c^{2}=a^{2}+b^{2}$
\end_inset

.
\end_layout

\begin_layout Enumerate
צ"ל: 
\begin_inset Formula $\norm{\gamma'\left(t\right)}=1$
\end_inset

.
\begin_inset Formula 
\[
\gamma'\left(t\right)=\left(-\frac{a}{c}\sin\left(\frac{t}{c}\right),\frac{a}{c}\cos\left(\frac{t}{c}\right),\frac{b}{c}\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\norm{\gamma'\left(t\right)} & =\sqrt{\frac{a^{2}}{c^{2}}\sin^{2}\left(\frac{t}{c}\right)+\frac{a^{2}}{c^{2}}\cos^{2}\left(\frac{t}{c}\right)+\frac{b^{2}}{c^{2}}}=\\
= & \sqrt{\frac{a^{2}}{c^{2}}\cdot1+\frac{b^{2}}{c^{2}}}=\sqrt{\frac{a^{2}+b^{2}}{c^{2}}}=1
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
T\left(t\right)=\gamma'\left(t\right)=\left(-\frac{a}{c}\sin\left(\frac{t}{c}\right),\frac{a}{c}\cos\left(\frac{t}{c}\right),\frac{b}{c}\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
N\left(t\right) & =\frac{\gamma''\left(t\right)}{\norm{\gamma''\left(t\right)}}=\frac{1}{\norm{\gamma''\left(t\right)}}\cdot\left(-\frac{a}{c^{2}}\cos\left(\frac{t}{c}\right),-\frac{a}{c^{2}}\sin\left(\frac{t}{c}\right),0\right)=\\
= & \frac{\left(-\frac{a}{c^{2}}\cos\left(\frac{t}{c}\right),-\frac{a}{c^{2}}\sin\left(\frac{t}{c}\right),0\right)}{\frac{a}{c^{2}}}=\left(-\cos\left(\frac{t}{c}\right),-\sin\left(\frac{t}{c}\right),0\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
B\left(t\right) & =T\left(t\right)\times N\left(t\right)=\left(\frac{b}{c}\sin\left(\frac{t}{c}\right),-\frac{b}{c}\cos\left(\frac{t}{c}\right),\frac{a}{c}\sin^{2}\left(\frac{t}{c}\right)+\frac{a}{c}\cos^{2}\left(\frac{t}{c}\right)\right)=\\
= & \left(\frac{b}{c}\sin\left(\frac{t}{c}\right),-\frac{b}{c}\cos\left(\frac{t}{c}\right),\frac{a}{c}\right)
\end{align*}

\end_inset

כעת, 
\begin_inset Formula $k\left(t\right)=\frac{a}{c^{2}}$
\end_inset

 ו-
\begin_inset Formula 
\[
B'\left(t\right)=\left(\frac{b}{c^{2}}\cos\left(\frac{t}{c}\right),\frac{b}{c^{2}}\sin\left(\frac{t}{c}\right),0\right)
\]

\end_inset

לכן 
\begin_inset Formula $\tau\left(t\right)=-\frac{b}{c^{2}}$
\end_inset

.
\end_layout

\begin_layout Enumerate
משוואת המישור המשיק בנק' 
