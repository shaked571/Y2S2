, 
\begin_inset Formula $a,b\in\mathbb{R}$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\gamma'\left(s\right)=\left(-a\sin\left(s\right),a\cos\left(s\right),b\right)$
\end_inset

.
\begin_inset Formula 
\[
\Vert\gamma'\left(s\right)\Vert=\sqrt{a^{2}\sin^{2}\left(s\right)+a^{2}\cos^{2}\left(s\right)+b}=\sqrt{a^{2}+b^{2}}>0
\]

\end_inset

בהנחה ש-
\begin_inset Formula $a,b\neq0$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
L\left(\gamma\right)=\int_{0}^{1}\Vert\gamma'\left(s\right)\Vert ds=\int_{0}^{1}\sqrt{a^{2}+b^{2}}ds=\sqrt{a^{2}+b^{2}}
\]

\end_inset


\end_layout

\begin_layout Enumerate
נגדיר 
\begin_inset Formula $f\colon\left[0,\sqrt{a^{2}+b^{2}}\right]\to\left[0,1\right]$
\end_inset

 כך ש-
\begin_inset Formula $f\left(t\right)=\frac{t}{\sqrt{a^{2}+b^{2}}}$
\end_inset

.
 אז
\begin_inset Formula 
\[
\Vert\left(\gamma\circ f\right)'\left(s\right)\Vert=\Vert\gamma'\left(f\left(s\right)\right)\circ f'\left(s\right)\Vert=\frac{1}{\sqrt{a^{2}+b^{2}}}\Vert\gamma'\left(f\left(s\right)\right)\Vert=\frac{\sqrt{a^{2}+b^{2}}}{\sqrt{a^{2}+b^{2}}}=1
\]

\end_inset

כנדרש.
