\begin_deeper
\begin_layout Standard
\begin_inset Formula $\alpha'\left(t\right)=\left(ae^{bt}\left(b\cdot\cos t-\sin t\right),ae^{bt}\left(b\sin t+\cos t\right)\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\Vert\alpha'\left(t\right)\Vert & =\left|ae^{bt}\right|\cdot\sqrt{b^{2}\cos^{2}t-2b\cos t\sin t+\sin^{2}t+b^{2}\sin^{2}t+2b\cos t\sin t+\cos^{2}t}=\\
= & \left|ae^{bt}\right|\sqrt{b^{2}+1}\xrightarrow[t\to\infty]{}0
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula 
\begin{align*}
\lim_{t\to\infty}\int_{t_{0}}^{t}\Vert a'\left(t\right)\Vert dt & =\lim_{t\to\infty}\int_{t_{0}}^{t}\left|ae^{bt}\right|\cdot\sqrt{b^{2}+1}dt=\\
= & \lim_{t\to\infty}a\sqrt{b^{2}+1}\int_{t_{0}}^{t}e^{bt}dt=\\
= & a\sqrt{b^{2}+1}\left[\frac{e^{bt}}{b}\right]_{t0}^{t}=\\
= & \frac{a}{b}\sqrt{b^{2}+1}\left(e^{bt}-e^{bt_{0}}\right)\xrightarrow[t\to\infty]{}-\frac{a}{b}\sqrt{b^{2}+1}e^{bt_{0}}
\end{align*}

\end_inset

