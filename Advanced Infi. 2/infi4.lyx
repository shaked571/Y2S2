\begin_layout Standard
\begin_inset Formula $\alpha\colon I\to\mathbb{R}^{3}$
\end_inset

 מסילה גזירה ברציפות.
 
\begin_inset Formula $\left[a,b\right]\subset I$
\end_inset

 ונסמן 
\begin_inset Formula $p=\alpha\left(a\right),\,q=\alpha\left(b\right)$
\end_inset

.
\end_layout

\begin_layout Enumerate
נשים לב כי לכל 
\begin_inset Formula $t\in\left[a,b\right]$
\end_inset


\begin_inset Formula 
\[
\alpha'\left(t\right)\cdot\hat{v}\leq\Vert\alpha'\left(t\right)\Vert\cdot\Vert\hat{v}\Vert=\Vert\alpha'\left(t\right)\Vert=\cdot1
\]

\end_inset

מא"ש קושי-שוורץ.
 לכן 
\begin_inset Formula 
\[
\int_{a}^{b}\alpha'\left(t\right)\cdot\hat{v}dt\leq\int_{a}^{b}\Vert\alpha'\left(t\right)\Vert dt
\]

\end_inset

 בנוסף, נגדיר 
\begin_inset Formula $f\left(t\right)=\alpha\left(t\right)\cdot\hat{v}=\sum_{i=1}^{3}\alpha_{i}\left(t\right)\cdot\hat{v}_{i}$
\end_inset

.
 אז 
\begin_inset Formula 
\[
f'\left(t\right)=\sum_{i=1}^{3}\alpha_{i}'\left(t\right)\cdot\hat{v}_{i}=\alpha'\left(t\right)\cdot\hat{v}
\]

\end_inset

בפרט זו פונק' רציפה ולכן מהמשפט היסודי
\begin_inset Formula 
\[
\left(q-p\right)\cdot\hat{v}=\alpha\left(b\right)\cdot\hat{v}-\alpha\left(a\right)\cdot\hat{v}=f\left(b\right)-f\left(a\right)=\int_{a}^{b}f'\left(t\right)dt=\int_{a}^{b}\alpha'\left(t\right)\hat{v}dt
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\hat{u}=\frac{q-p}{\Vert q-p\Vert}$
\end_inset

.
\begin_inset Formula 
\[
\left(q-p\right)\cdot\hat{u}=\left(q-p\right)\cdot\frac{\left(q-p\right)}{\Vert q-p\Vert}=\frac{\Vert q-p\Vert^{2}}{\Vert q-p\Vert}=\Vert q-p\Vert=\left|\alpha\left(b\right)-\alpha\left(a\right)\right|
\]

\end_inset

השאר נובע מסעיף א'.
